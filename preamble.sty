\newcommand{\SSpsetQ}[1]
{
  \begin{mdframed}
    [
      style = SSqbox,
      innertopmargin = 10pt,
      innerbottommargin = 10pt
    ]
      \begin{SSpsetQbox}#1\end{SSpsetQbox}
  \end{mdframed}
}

